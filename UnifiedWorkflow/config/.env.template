# AI Workflow Engine Environment Configuration
# Non-sensitive configuration only - secrets are handled via Docker secrets
ENVIRONMENT=production
DEBUG=false

# Domain Configuration for DDNS
DOMAIN=aiwfe.com

# Database Configuration (passwords via Docker secrets)
POSTGRES_USER=app_user
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=ai_workflow_db

# Redis Configuration (passwords via Docker secrets)
REDIS_HOST=redis
REDIS_PORT=6379

# Qdrant Configuration (API key via Docker secrets)
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# Ollama Configuration
OLLAMA_API_BASE_URL=http://ollama:11434
OLLAMA_GENERATION_MODEL_NAME=llama3.2:3b
OLLAMA_EMBEDDING_MODEL_NAME=nomic-embed-text

# CORS Configuration (will be updated by setup script)
CORS_ALLOWED_ORIGINS=https://localhost,https://127.0.0.1,https://aiwfe.com,http://aiwfe.com