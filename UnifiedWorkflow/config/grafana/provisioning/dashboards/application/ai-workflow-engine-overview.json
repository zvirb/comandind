{
  "dashboard": {
    "id": null,
    "title": "AI Workflow Engine - System Overview",
    "tags": ["ai-workflow-engine", "overview", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 39,
    "version": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Status Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=~\"fastapi-api|webui|worker|postgres-exporter|redis-exporter|qdrant\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "min": 0,
            "max": 1,
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 2,
        "title": "Request Rate (req/sec)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"fastapi-api\"}[5m])",
            "legendFormat": "API Requests",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"unit": "reqps", "min": 0},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"fastapi-api\"}[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"fastapi-api\"}[5m]))",
            "legendFormat": "50th Percentile (Median)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"unit": "s", "min": 0},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 4,
        "title": "Error Rate (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "(rate(http_requests_total{job=\"fastapi-api\",status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"fastapi-api\"}[5m])) * 100",
            "legendFormat": "5xx Error Rate",
            "refId": "A"
          },
          {
            "expr": "(rate(http_requests_total{job=\"fastapi-api\",status=~\"4..\"}[5m]) / rate(http_requests_total{job=\"fastapi-api\"}[5m])) * 100",
            "legendFormat": "4xx Error Rate",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"unit": "percent", "min": 0},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true},
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "title": "Active Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "Active Connections - {{datname}}",
            "refId": "A"
          },
          {
            "expr": "pg_settings_max_connections",
            "legendFormat": "Max Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"unit": "short", "min": 0},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 6,
        "title": "Redis Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Used Memory",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes",
            "legendFormat": "Max Memory",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"unit": "bytes", "min": 0},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 7,
        "title": "Container CPU Usage (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"ai_workflow_engine.*\",name!=\"\"}[5m]) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"unit": "percent", "min": 0, "max": 100},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 8,
        "title": "Container Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\"ai_workflow_engine.*\",name!=\"\"}",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"unit": "bytes", "min": 0},
          {"show": false}
        ],
        "legend": {"show": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 9,
        "title": "Worker Queue Depth",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "celery_task_sent_total - celery_task_succeeded_total - celery_task_failed_total - celery_task_rejected_total - celery_task_revoked_total",
            "legendFormat": "Pending Tasks",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "orange", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 10,
        "title": "Authentication Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 32},
        "targets": [
          {
            "expr": "(rate(auth_attempts_total{status=\"success\"}[10m]) / rate(auth_attempts_total[10m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 80},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 11,
        "title": "Active Alerts",
        "type": "table",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "list",
              "filterable": true
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "severity"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "mappings",
                  "value": [
                    {"options": {"critical": {"color": "red", "index": 0}}, "type": "value"},
                    {"options": {"high": {"color": "orange", "index": 1}}, "type": "value"},
                    {"options": {"medium": {"color": "yellow", "index": 2}}, "type": "value"},
                    {"options": {"low": {"color": "green", "index": 3}}, "type": "value"}
                  ]
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "job": true
              },
              "renameByName": {
                "alertname": "Alert",
                "instance": "Instance",
                "severity": "Severity",
                "summary": "Summary"
              }
            }
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "deployment_timestamp",
          "iconColor": "blue",
          "titleFormat": "Deployment"
        }
      ]
    },
    "variables": [
      {
        "name": "instance",
        "type": "query",
        "label": "Instance",
        "query": "label_values(up, instance)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  }
}