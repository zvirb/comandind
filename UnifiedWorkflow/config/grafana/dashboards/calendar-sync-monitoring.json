{
  "dashboard": {
    "id": null,
    "title": "Calendar Sync Monitoring",
    "tags": ["calendar", "sync", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Calendar Sync Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "calendar_sync_health_score{time_window=\"1h\"}",
            "legendFormat": "Health Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "calendar_sync:success_rate_5m",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Schema Errors (Critical)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(calendar_sync_schema_errors_total[1h])",
            "legendFormat": "Schema Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "targets": [
          {
            "expr": "count(calendar_sync_circuit_breaker_state == 1)",
            "legendFormat": "Open Breakers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Calendar Sync Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(calendar_sync_requests_total[5m])",
            "legendFormat": "{{ status }} requests/sec"
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true,
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Error Rate by Pattern",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(calendar_sync_requests_total{status=~\"failure|schema_error|auth_error\"}[5m]) by (error_pattern)",
            "legendFormat": "{{ error_pattern }}"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Database Schema Errors Detail",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(calendar_sync_schema_errors_total[5m]) by (error_type, table_name, column_name)",
            "legendFormat": "{{ error_type }}: {{ table_name }}.{{ column_name }}"
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [0], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "0m",
          "frequency": "10s",
          "handler": 1,
          "name": "Schema Error Alert",
          "noDataState": "no_data"
        },
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(calendar_sync_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(calendar_sync_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(calendar_sync_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          },
          {
            "expr": "calendar_sync_baseline_duration_seconds",
            "legendFormat": "Baseline"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "show": true,
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 26}
      },
      {
        "id": 9,
        "title": "Events Synced Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(calendar_sync_events_synced_sum[5m]) / rate(calendar_sync_events_synced_count[5m])",
            "legendFormat": "Avg Events/Sync"
          },
          {
            "expr": "histogram_quantile(0.95, rate(calendar_sync_events_synced_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Events",
            "show": true,
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 26}
      },
      {
        "id": 10,
        "title": "OAuth Token Scope Errors",
        "type": "table",
        "targets": [
          {
            "expr": "increase(calendar_sync_oauth_token_errors_total{error_subtype=\"missing_scope_column\"}[1h]) by (user_id)",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "filterable": true
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {"id": "displayName", "value": "Error Count"}
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 35}
      },
      {
        "id": 11,
        "title": "Circuit Breaker States",
        "type": "table",
        "targets": [
          {
            "expr": "calendar_sync_circuit_breaker_state",
            "format": "table",
            "instant": true
          },
          {
            "expr": "calendar_sync_consecutive_failures",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value #A"},
              "properties": [
                {"id": "displayName", "value": "Breaker State"},
                {"id": "mappings", "value": [
                  {"options": {"0": {"text": "Closed"}}, "type": "value"},
                  {"options": {"1": {"text": "Open"}}, "type": "value"}
                ]}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value #B"},
              "properties": [
                {"id": "displayName", "value": "Failures"}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 35}
      },
      {
        "id": 12,
        "title": "Recent Critical Alerts",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"api\"} |= \"calendar\" |= \"schema_error\" | json",
            "legendFormat": ""
          }
        ],
        "options": {
          "showLabels": true,
          "showTime": true,
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 43}
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Schema Error Alerts",
          "datasource": "prometheus",
          "enable": true,
          "hide": false,
          "iconColor": "red",
          "query": "ALERTS{alertname=\"CalendarSyncSchemaError\"}",
          "titleFormat": "Schema Error",
          "textFormat": "Database schema error detected"
        },
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "enable": true,
          "hide": false,
          "iconColor": "green",
          "query": "up{job=\"api\"} offset 1m",
          "titleFormat": "Deployment",
          "textFormat": "Service restart detected"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "user_id",
          "type": "query",
          "query": "label_values(calendar_sync_requests_total, user_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "endpoint",
          "type": "query",
          "query": "label_values(calendar_sync_requests_total, endpoint)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}