{
  "dashboard": {
    "id": null,
    "title": "AIWFE Consolidated Services Overview",
    "tags": ["aiwfe", "consolidated", "overview", "production"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(up{job=~\"aiwfe-.*\"}) / count(up{job=~\"aiwfe-.*\"}) * 100",
            "legendFormat": "Service Availability %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Production Site Status",
        "type": "stat",
        "targets": [
          {
            "expr": "probe_success{job=\"production-health\", instance=\"https://aiwfe.com\"}",
            "legendFormat": "AIWFE.com"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Cost Savings Progress",
        "type": "stat",
        "targets": [
          {
            "expr": "cost:annual_savings_projection_usd",
            "legendFormat": "Annual Savings"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 300000},
                {"color": "green", "value": 400000}
              ]
            },
            "unit": "currencyUSD",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "WebSocket Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket:avg_connection_time_5m",
            "legendFormat": "Avg Connection Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 20},
                {"color": "red", "value": 30}
              ]
            },
            "unit": "s",
            "max": 30
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Service Availability Matrix",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"aiwfe-api-gateway\"}",
            "legendFormat": "API Gateway"
          },
          {
            "expr": "up{job=\"aiwfe-webui\"}",
            "legendFormat": "WebUI"
          },
          {
            "expr": "up{job=\"integration-service\"}",
            "legendFormat": "Integration"
          },
          {
            "expr": "up{job=\"reasoning-service\"}",
            "legendFormat": "Reasoning"
          },
          {
            "expr": "up{job=\"metrics-aggregator\"}",
            "legendFormat": "Metrics"
          },
          {
            "expr": "up{job=\"ollama\"}",
            "legendFormat": "Ollama"
          },
          {
            "expr": "up{job=\"postgres-exporter\"}",
            "legendFormat": "PostgreSQL"
          },
          {
            "expr": "up{job=\"redis-exporter\"}",
            "legendFormat": "Redis"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ]
          }
        },
        "options": {
          "colorMode": "background"
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Resource Utilization - CPU",
        "type": "timeseries",
        "targets": [
          {
            "expr": "consolidation:cpu_efficiency",
            "legendFormat": "CPU Efficiency %"
          },
          {
            "expr": "70",
            "legendFormat": "Target Min (70%)"
          },
          {
            "expr": "80",
            "legendFormat": "Target Max (80%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Resource Utilization - Memory",
        "type": "timeseries",
        "targets": [
          {
            "expr": "consolidation:memory_efficiency",
            "legendFormat": "Memory Efficiency %"
          },
          {
            "expr": "70",
            "legendFormat": "Target Min (70%)"
          },
          {
            "expr": "80",
            "legendFormat": "Target Max (80%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Daily Cost Breakdown",
        "type": "piechart",
        "targets": [
          {
            "expr": "cost:daily_cpu_cost_usd",
            "legendFormat": "CPU Costs"
          },
          {
            "expr": "cost:daily_memory_cost_usd",
            "legendFormat": "Memory Costs"
          },
          {
            "expr": "cost:daily_storage_cost_usd",
            "legendFormat": "Storage Costs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "SSL Certificate Status",
        "type": "stat",
        "targets": [
          {
            "expr": "(probe_ssl_earliest_cert_expiry{instance=\"aiwfe.com:443\"} - time()) / 86400",
            "legendFormat": "Days Until Expiry"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 7},
                {"color": "green", "value": 30}
              ]
            },
            "unit": "days"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"aiwfe-api-gateway\"}[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"aiwfe-api-gateway\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"aiwfe-api-gateway\"}[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 11,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(pg_stat_database_xact_commit[5m])",
            "legendFormat": "Commits/sec"
          },
          {
            "expr": "rate(pg_stat_database_xact_rollback[5m])",
            "legendFormat": "Rollbacks/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 12,
        "title": "Redis Cache Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients"
          },
          {
            "expr": "rate(redis_keyspace_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(redis_keyspace_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      }
    ]
  }
}